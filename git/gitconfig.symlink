[user]
        name = Ankush Agarwal
        email = ankushagarwal11@gmail.com
[credential]
        helper = osxkeychain
[alias]
        promote = !$ZSH/bin/git-promote
        wtf     = !$ZSH/bin/git-wtf
        rank-contributors = !$ZSH/bin/git-rank-contributors
        count   = !git shortlog -sn
        co = checkout
        c = commit
        st = status
        br = branch
        hist = log --all --oneline --graph --pretty=format:'%h %Cgreen%ar %Creset|%s %Cred%d %Creset[%an]' --decorate --date=short
        h = log --all --oneline --graph --pretty=format:'%h %Cgreen%ar %Creset|%s %Cred%d %Creset[%an]' --decorate --date=short
        type = cat-file -t
        dump = cat-file -p
        d = diff --ignore-space-at-eol -b -w --word-diff=color
        g = grep --break --heading --line-number -e
[grep]
        extendRegexp = true
        lineNumber = true
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red
        new = green
[color "status"]
        added = yellow
        changed = green
        untracked = cyan

[core]
  excludesfile = /Users/ankushagarwal/.gitignore
  editor = vim
[push]
        default = matching
[apply]
        whitespace = nowarn
[help]
        autocorrect = 1
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
